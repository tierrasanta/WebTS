@model TierraSanta.Models.PlantillaCultivoDetalle

@{
    ViewBag.Title = "Eliminar";
}



<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Eliminar PlantillaCultivoDetalle</h3>
                </div>
                <div class="box-body">
                    <h4 class="text-danger">¿Estás seguro que quieres eliminar esto?</h4>
                    <dl class="dl-horizontal">
                        <dt>
                            Plantilla
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PlantillaCultivoCabecera.descripcion)
                        </dd>

                        <dt>
                            Actividad
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.TablaActividades.descripcion)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.cantidad)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.cantidad)
                        </dd>

                        <dt>
                            Fecha de creacion
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechacreacion)
                        </dd>

                        <dt>
                            Fecha de cambio
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechacambio)
                        </dd>



                    </dl>

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-actions no-color">
                            <input type="submit" value="Delete" class="btn btn-primary" />
                            @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-default" })
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
