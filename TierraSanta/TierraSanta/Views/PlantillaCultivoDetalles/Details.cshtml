@model TierraSanta.Models.PlantillaCultivoDetalle

@{
    ViewBag.Title = "Detalle";
}



<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">PlantillaCultivoDetalle</h3>
                </div>
                <div class="box-body">
                    <dl class="dl-horizontal">

                        <dt>
                            Plantilla
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PlantillaCultivoCabecera.descripcion)
                        </dd>

                        <dt>
                            Actividad
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.TablaActividades.descripcion)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.cantidad)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.cantidad)
                        </dd>

                        <dt>
                            Fecha de creacion
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechacreacion)
                        </dd>

                        <dt>
                            Fecha de cambio
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechacambio)
                        </dd>

                    </dl>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">

                            @Html.ActionLink("Edita", "Edit", new { id = Model.idempresa, id2 = Model.idplantilla, id3 = Model.idactividad }, new { @class = "btn btn-primary" })
                            @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-default" })


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>




