@model WebTS2.Models.Cultivo

@{
    ViewBag.Title = "Eliminar";
}



<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Eliminar Cultivo</h3>
                </div>
                <div class="box-body">
                    <h4 class="text-danger">¿Estás seguro que quieres eliminar esto?</h4>
                    <dl class="dl-horizontal">

                        <dt>
                            Fundo
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Fundo.descripcion)
                        </dd>

                        <dt>
                            Lote
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Lote.descripcion)
                        </dd>

                        <dt>
                            Plantilla
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PlantillaCultivoCabecera.descripcion)
                        </dd>
                        <dt>
                            Area
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Lote.area)
                        </dd>

                        <dt>
                            Fecha de inicio
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechainicio)
                        </dd>

                        <dt>
                            Fecha final
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechafin)
                        </dd>

                        <dt>
                            Fecha de creacion
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechacreacion)
                        </dd>

                        <dt>
                            Fecha de cambio
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.fechacambio)
                        </dd>



                    </dl>

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-actions no-color">
                            <input type="submit" value="Delete" class="btn btn-primary" />
                            @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-default" })
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
