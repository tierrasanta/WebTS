@model WebTS2.Models.PlantillaCultivoDetalle

@{
    ViewBag.Title = "Agregar";
}



<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Agregar PlantillaCultivoDetalle</h3>
                </div>
                <div class="box-body">

                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">

                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                            <div class="form-group">
                                <label for="idlote" class="control-label col-md-2">Lote</label>
                                <div class="col-md-10">
                                    @Html.DropDownList("idlote", null, htmlAttributes: new { @class = "form-control" })

                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Grabar" class="btn btn-primary" />
                                    @Html.ActionLink("Grabar", "Create", null, new { @class = "btn btn-default" }))
                                    @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-default" })
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
